<div class="row">
    <div class="col-md-6">
        <div class="mb-3">
            <label class="form-label">Nom</label>
            {{ form_widget(form.nom,{attr: {class: 'form-control form-control-sm'}}) }}
        </div>
    </div>
    <div class="col-md-6">
        <div class="mb-3">
            <label class="form-label">Prénoms</label>
            {{ form_widget(form.prenoms,{attr: {class: 'form-control form-control-sm'}}) }}
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="mb-3">
            <label class="form-label">Telephone</label>
            {{ form_widget(form.telephone,{attr: {class: 'form-control form-control-sm'}}) }}
        </div>
    </div>
    <div class="col-md-6">
        <div class="mb-3">
            <label class="form-label">Nom de l'entreprise</label>
            {{ form_widget(form.entreprise,{attr: {class: 'form-control form-control-sm'}}) }}
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="mb-3">
            <label class="form-label">Email</label>
            {{ form_widget(form.email,{attr: {class: 'form-control form-control-sm'}}) }}
        </div>
    </div>
    <div class="col-md-6">
        <div class="mb-3">
            <label class="form-label">Date de naissance</label>
            {{ form_widget(form.dateNaissance,{attr: {class: 'form-control form-control-sm'}}) }}
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="mb-3">
            <label class="form-label">Categorie</label>
            {{ form_widget(form.categorie,{attr: {class: 'form-control select2'}}) }}
        </div>
    </div>
    <div class="col-md-6">
        <div class="mb-3">
            <label class="form-label">Commune</label>
            {{ form_widget(form.commune,{attr: {class: 'form-control form-control-sm'}}) }}
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="mb-3">
            <label class="form-label">Description de l'activité de l'entreprise</label>
            {{ form_widget(form.description,{attr: {class: 'form-control form-control-sm'}}) }}
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="mb-3">
            <label class="form-label">Etiez vous à la première Edition du DEC ?</label>
            {{ form_widget(form.premiere_edition,{attr: {class: 'form-control select2'}}) }}
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="mb-3">
            <label class="form-label">Nature/type d'entreprise</label>
            {{ form_widget(form.typeEntreprise,{attr: {class: 'form-control select2'}}) }}
        </div>
    </div>
    <div class="col-md-6">
        <div class="mb-3">
            <label class="form-label">Chiffre d'affaire</label>
            {{ form_widget(form.caEntreprise,{attr: {class: 'form-control separator form-control-sm'}}) }}
        </div>
    </div>
</div>